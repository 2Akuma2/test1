# Command line options for qftest

# This is no longer needed
dynamic =		false

# The license file, default is <qftest-dir>/license or ~/.qftest/license
#
#license =		/somewhere/license

# The logging system
# general
# log-default =		7
# logoutputlevel =	2
# logprequeuelevel =	4
# logflushsize =	30
# logflushlevel =	3
# logpostflush =	3
# log files
# logfile =		qftest.log
logfilesize =		500
logfilenum =		5
logfilelevel =		10
# log server
#logserver =		qflog
#logregistry =		true
#logcreateregistry =	true
#loglocalonly =		true


log-default =		4
logoutputlevel =	2
logprequeuelevel =	10
logqueueing =		true
