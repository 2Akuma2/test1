  <PackageRoot id="_4">
    <Package id="_AE" name="abhängigkeiten">
      <Dependency id="_7b" name="SUT gestartet">
        <SetupSequence id="_7c" name="starte SUT">
          <ProcedureCall id="_3Ff" procedure="startStop.starteApplikation"/>
        </SetupSequence>
        <CleanupSequence id="_7e" name="stoppe SUT">
          <TryStep id="_6VM" name="SUT bereits beendet?">
            <ProcessWaiter client="$(client)" id="_6VN" timeout="1"/>
            <CatchSequence exception="TestException" id="_6VO" maxerror="0"
                           name="SUT nicht beendet -&gt; beenden">
              <ProcedureCall id="_6VP"
                             procedure="startStop.stoppeApplikation"/>
            </CatchSequence>
          </TryStep>
        </CleanupSequence>
      </Dependency>
    </Package>
    <Package id="_3FB" name="startStop">
      <Procedure id="_3FC" name="starteApplikation">
        <SetGlobalStep id="_1p6F" varname="client">
          <default>carconfig</default>
        </SetGlobalStep>
        <ClientWaiter client="$(client)" id="_1p6G" local="true" raise="false"
                      resvarname="isSUTRunning" timeout="1">
          <comment>Dieser Knoten prüft, ob das SUT bereits läuft. Das Ergebnis der Prüfung wird in der Variable isSUTRunning gespeichert. Diese Variable enthält entweder true, wenn das SUT läuft oder false, wenn das SUT nicht läuft. Im folgenden IF-Knoten wird diese Variable ausgewertet.</comment>
        </ClientWaiter>
        <IfSequence id="_1p6H" name="SUT starten, wenn notwendig"
                    test="not $(isSUTRunning)">
          <IfSequence id="_3FD" name="auf Windows" test="${qftest:windows}">
            <SUTClientStarter client="$(client)"
                              directory="${qftest:dir.version}/demo/carconfig"
                              executable="${qftest:dir.version}/demo/carconfig/CarConfig.cmd"
                              id="_3FE">
              <parameter>de</parameter>
            </SUTClientStarter>
            <ElseSequence id="_3FF" name="auf Unixsystemen">
              <SUTClientStarter client="$(client)"
                                directory="${qftest:dir.version}/demo/carconfig"
                                executable="${qftest:dir.version}/demo/carconfig/CarConfig.sh"
                                id="_3FG">
                <parameter>de</parameter>
              </SUTClientStarter>
            </ElseSequence>
          </IfSequence>
          <ClientWaiter client="$(client)" id="_3FH"/>
          <ComponentWaiter client="$(client)" component="JCarConfigurator"
                           id="_3FI" timeout="30000"/>
        </IfSequence>
      </Procedure>
      <Procedure id="_3FJ" name="stoppeApplikation">
        <TryStep id="_6Vb" name="versuchen SUT normal zu beenden">
          <ProcedureCall id="_5g" procedure="menü.datei.beenden"/>
          <CatchSequence exception="TestException" id="_6Vc" maxerror="0"
                         name="normales Ende nicht möglich -&gt; abschiessen">
            <ClientStopper client="$(client)" id="_6Vd"/>
          </CatchSequence>
        </TryStep>
        <ProcessWaiter client="$(client)" id="_3FL"/>
      </Procedure>
    </Package>
    <Package id="_32" name="menü">
      <Package id="_3H" name="datei">
        <Procedure id="_3I" name="zurücksetzen">
          <MouseEventStep clicks="1" client="$(client)" component="mFile"
                          event="MOUSE_MPRC" id="_3J" modifiers="16" x="1"
                          y="1"/>
          <MouseEventStep clicks="1" client="$(client)" component="miReset"
                          event="MOUSE_MPRC" id="_3K" modifiers="16" x="1"
                          y="1"/>
        </Procedure>
        <Procedure id="_5Z" name="beenden">
          <MouseEventStep clicks="1" client="$(client)" component="mFile"
                          event="MOUSE_MPRC" id="_5a" modifiers="16" x="1"
                          y="1"/>
          <MouseEventStep clicks="1" client="$(client)" component="miExit"
                          event="MOUSE_MPRC" id="_5b" modifiers="16" x="1"
                          y="1"/>
        </Procedure>
      </Package>
      <Package id="_3DN" name="einstellungen">
        <Procedure id="_3DJ" name="öffne Fahrzeugdialog">
          <MouseEventStep clicks="1" client="$(client)" component="mOptions"
                          event="MOUSE_MPRC" id="_3DK" modifiers="16" x="1"
                          y="1"/>
          <MouseEventStep clicks="1" client="$(client)" component="miVehicles"
                          event="MOUSE_MPRC" id="_3DL" modifiers="16" x="1"
                          y="1"/>
          <ComponentWaiter client="$(client)" component="VehiclesDialog"
                           id="_3DM"/>
        </Procedure>
        <Procedure id="_3DO" name="öffne Zubehördialog">
          <MouseEventStep clicks="1" client="$(client)" component="mOptions"
                          event="MOUSE_MPRC" id="_3DP" modifiers="16" x="1"
                          y="1"/>
          <MouseEventStep clicks="1" client="$(client)"
                          component="miAccessories" event="MOUSE_MPRC"
                          id="_3DQ" modifiers="16" x="1" y="1"/>
          <ComponentWaiter client="$(client)" component="AccessoryDialog"
                           id="_3DR"/>
        </Procedure>
      </Package>
    </Package>
    <Package id="_4B" name="fahrzeugePanel">
      <Procedure id="_4C" name="wähleTab">
        <MouseEventStep clicks="1" client="$(client)"
                        component="TabbedPane@Fahrzeuge" event="MOUSE_MPRC"
                        id="_4H" modifiers="16" x="1" y="1"/>
      </Procedure>
      <Procedure id="_4E" name="wähleFahrzeug">
        <variable name="modell"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="VehicleTable@Modell@$(modell)"
                        event="MOUSE_MPRC" id="_4F" modifiers="16" x="1"
                        y="0"/>
      </Procedure>
    </Package>
    <Package id="_4L" name="sondermodellePanel">
      <Procedure id="_4M" name="wähleTab">
        <MouseEventStep clicks="1" client="$(client)"
                        component="TabbedPane@Sondermodelle"
                        event="MOUSE_MPRC" id="_4S" modifiers="16" x="1"
                        y="1"/>
      </Procedure>
