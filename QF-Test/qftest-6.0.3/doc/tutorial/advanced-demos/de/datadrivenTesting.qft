  <PackageRoot id="_4">
    <Package id="_5i" name="startStop">
      <Procedure id="_47" name="starteApplikation">
        <SetGlobalStep id="_1p6F" varname="client">
          <default>carconfig</default>
        </SetGlobalStep>
        <ClientWaiter client="$(client)" id="_1p6G" local="true" raise="false"
                      resvarname="isSUTRunning" timeout="1">
          <comment>Dieser Knoten prüft, ob das SUT bereits läuft. Das Ergebnis der Prüfung wird in der Variable isSUTRunning gespeichert. Diese Variable enthält entweder true, wenn das SUT läuft oder false, wenn das SUT nicht läuft. Im folgenden IF-Knoten wird diese Variable ausgewertet.</comment>
        </ClientWaiter>
        <IfSequence id="_1p6H" name="SUT starten, wenn notwendig"
                    test="not $(isSUTRunning)">
          <IfSequence id="_9Z" name="auf Windows" test="${qftest:windows}">
            <SUTClientStarter client="$(client)"
                              directory="${qftest:dir.version}/demo/carconfig"
                              executable="${qftest:dir.version}/demo/carconfig/CarConfig.cmd"
                              id="_9a">
              <parameter>de</parameter>
            </SUTClientStarter>
            <ElseSequence id="_9b" name="auf Unixsystemen">
              <SUTClientStarter client="$(client)"
                                directory="${qftest:dir.version}/demo/carconfig"
                                executable="${qftest:dir.version}/demo/carconfig/CarConfig.sh"
                                id="_1p6I">
                <parameter>de</parameter>
              </SUTClientStarter>
            </ElseSequence>
          </IfSequence>
          <ClientWaiter client="$(client)" id="_49"/>
          <ComponentWaiter client="$(client)" component="JCarConfigurator"
                           id="_68" timeout="30000"/>
        </IfSequence>
      </Procedure>
      <Procedure id="_5d" name="stoppeApplikation">
        <ProcedureCall id="_5g" procedure="menü.datei.beenden"/>
        <ProcessWaiter client="$(client)" id="_5h"/>
      </Procedure>
    </Package>
    <Package id="_32" name="menü">
      <Package id="_3H" name="datei">
        <Procedure id="_3I" name="zurücksetzen">
          <MouseEventStep clicks="1" client="$(client)" component="mFile"
                          event="MOUSE_MPRC" id="_3J" modifiers="16" x="1"
                          y="1"/>
          <MouseEventStep clicks="1" client="$(client)" component="miReset"
                          event="MOUSE_MPRC" id="_3K" modifiers="16" x="1"
                          y="1"/>
        </Procedure>
        <Procedure id="_5Z" name="beenden">
          <MouseEventStep clicks="1" client="$(client)" component="mFile"
                          event="MOUSE_MPRC" id="_5a" modifiers="16" x="1"
                          y="1"/>
          <MouseEventStep clicks="1" client="$(client)" component="miExit"
                          event="MOUSE_MPRC" id="_5b" modifiers="16" x="1"
                          y="1"/>
        </Procedure>
      </Package>
    </Package>
    <Package id="_4B" name="fahrzeugePanel">
      <Procedure id="_4C" name="wähleTab">
        <MouseEventStep clicks="1" client="$(client)"
                        component="TabbedPane@Fahrzeuge" event="MOUSE_MPRC"
                        id="_4H" modifiers="16" x="1" y="1"/>
      </Procedure>
      <Procedure id="_4E" name="wähleModell">
        <variable name="modell"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="VehicleTable@Modell@$(modell)"
                        event="MOUSE_MPRC" id="_4F" modifiers="16" x="1"
                        y="0"/>
      </Procedure>
    </Package>
    <Package id="_4L" name="sondermodellePanel">
      <Procedure id="_4M" name="wähleTab">
        <MouseEventStep clicks="1" client="$(client)"
                        component="TabbedPane@Sondermodelle"
                        event="MOUSE_MPRC" id="_4S" modifiers="16" x="1"
                        y="1"/>
      </Procedure>
      <Procedure id="_4O" name="wähleModell">
        <variable name="modell"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="SpecialsCombo.button" event="MOUSE_MPRC"
                        id="_4W" modifiers="16" x="1" y="1"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="popupInvokerClass:_JComboBox.list@$(modell)"
                        event="MOUSE_MPRC" id="_4X" modifiers="16" x="1"
                        y="1"/>
      </Procedure>
    </Package>
    <Package id="_4b" name="zubehörPanel">
      <Procedure id="_4c" name="wähleTab">
        <MouseEventStep clicks="1" client="$(client)"
                        component="TabbedPane@Zubehör" event="MOUSE_MPRC"
                        id="_4j" modifiers="16" x="1" y="0"/>
      </Procedure>
      <Procedure id="_4u" name="setzeRabatt">
        <variable name="rabatt">0</variable>
        <TextInputStep clear="true" client="$(client)"
                       component="DiscountValue" id="_51">
          <text>$(rabatt)</text>
        </TextInputStep>
      </Procedure>
      <Procedure id="_4e" name="wähleZubehör">
        <variable name="zubehör"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="AccessoryTable@Beschreibung@$(zubehör)"
                        event="MOUSE_MPRC" id="_4o" modifiers="16" x="1"
                        y="1"/>
        <FetchIndexStep client="$(client)"
                        component="AccessoryTable@Beschreibung@$(zubehör)"
                        id="_4p" local="true" varname="rowIndex"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="AccessoryTable@ &amp;$(rowIndex)"
                        event="MOUSE_MPRC" id="_4q" modifiers="16" x="1"
                        y="1"/>
      </Procedure>
      <Procedure id="_53" name="prüfeEndpreis">
        <variable name="endpreis"/>
        <CheckStringStep client="$(client)" component="CalculatedPrice"
                         id="_55" regexp="true">
          <text>.*$(endpreis).*</text>
          <comment>Für die Prüfung muss ein regulärer Ausdruck verwendet werden, damit unterschiedliche
Währungen auch getestet werden können, z.B. &#8364; im Euroraum und SFr in der Schweiz.</comment>
        </CheckStringStep>
      </Procedure>
    </Package>
