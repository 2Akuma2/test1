      </Procedure>
      <Procedure id="_4O" name="selectSpecial">
        <variable name="special"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="SpecialsPanel.button" event="MOUSE_MPRC"
                        id="_4W" modifiers="16" x="1" y="1"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="listInvokerName:_SpecialsCombo@$(special)"
                        event="MOUSE_MPRC" id="_1p7S" modifiers="16" x="1"
