      <Procedure id="_2YQA" name="startWebswingServerLocal">
        <variable name="webswingdir"/>
        <comment>Start the Webswing server locally.

@param webswingdir 	the directory path of Webswing

@author qfs
@since 5.2.0 </comment>
        <TryStep id="_2YOA">
          <CommentStep id="_2YTd">
            <heading>&lt;font style="color:green"&gt;First check if a Webswing server process is already running</heading>
          </CommentStep>
          <ShellClientStarter client="webswingServer" command="echo"
                              id="_2YTb"/>
          <ProcessWaiter client="webswingServer" id="_2YTc"/>
          <CommentStep id="_2YTY">
            <heading>&lt;font style="color:green"&gt;Intially start Webswing server as "$(client)"</heading>
          </CommentStep>
          <CommentStep id="_2YTZ">
            <heading>&lt;font style="color:green"&gt;That way sub-processes of the Webswing server will connect with that name</heading>
          </CommentStep>
          <SUTClientStarter client="$(client)" directory="$(webswingdir)"
                            executable="${default:javaexe:${qftest:java}}"
                            id="_2YOD">
            <parameter>-jar</parameter>
            <parameter>$(executable)</parameter>
            <parameter>-j</parameter>
            <parameter>jetty.properties</parameter>
            <extraarg>webswing=true</extraarg>
          </SUTClientStarter>
          <CommentStep id="_2YTa">
            <heading>&lt;font style="color:green"&gt;Then rename the Webswing server process to "webswingServer"</heading>
          </CommentStep>
          <ProcedureCall id="_2YTe" procedure="qfs.client.renameClient">
            <variable name="oldName">$(client)</variable>
            <variable name="newName">webswingServer</variable>
            <variable name="trueName">webswingServer</variable>
          </ProcedureCall>
          <IfSequence id="_2YOJ" name="Wait for server startup"
                      test="rc.lookup(&#34;webswingServerStartupMessage&#34;)">
            <ProcedureCall id="_2YOK"
                           procedure="qfs.utils.waitForClientOutput">
              <variable name="client">webswingServer</variable>
              <variable name="expectedOutput">$(webswingServerStartupMessage)</variable>
              <variable name="timeout">${default:timeout:30000}</variable>
              <variable name="polling">1000</variable>
            </ProcedureCall>
            <ElseSequence id="_2YOL">
              <ProcedureCall id="_2YOM" procedure="qfs.utils.sleep">
                <variable name="sleeptime">10000</variable>
              </ProcedureCall>
            </ElseSequence>
          </IfSequence>
          <CatchSequence exception="DuplicateClientException" id="_2YON"
                         maxerror="0">
            <CommentStep id="_2YOO">
              <heading>Process already runnig</heading>
            </CommentStep>
          </CatchSequence>
        </TryStep>
      </Procedure>
      <Procedure id="_3N6" name="downloadWebswingLocal">
        <comment>Startet die Electron-App</comment>
        <BasicSequence id="_2YUw" name="downloadWebswing">
          <SetGlobalStep id="_2YUy" local="true" varname="demoSource">
            <default>https://www.qfs.de/webswing-demo-$(webswingversion)</default>
