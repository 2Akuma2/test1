  <PackageRoot id="_3">
    <Package id="_2YHr" name="dependencies">
      <Dependency id="_2YO5" name="webswingServerStarted">
        <variable name="webswingServer">Webswing</variable>
        <variable name="webswingServerStartupMessage"/>
        <SetupSequence id="_2YO6" name="Start webswingServer">
          <BasicSequence id="_7" name="Set global variables">
            <SetGlobalStep id="_8" varname="client">
              <default>$(client)</default>
            </SetGlobalStep>
          </BasicSequence>
          <BasicSequence id="_2YO8" name="Download Webswing Demo if needed">
            <ProcedureCall id="_1pPF" local="true"
                           procedure="qfs.shellutils.exists"
                           retvarname="webswingExists">
              <variable name="file">$(webswingdir)/$(executable)</variable>
            </ProcedureCall>
            <IfSequence id="_2YQz" test="not $(webswingExists)">
              <ProcedureCall id="_2YRL" local="true"
                             procedure="qfs.dialogs.showYes_No_Cancel_Dialog"
                             retvarname="ret">
                <variable name="title">Download Webswing demo?</variable>
                <variable name="msg">No Webswing demo found
in $(webswingdir)

Download and install Webswing demo 
in this directory? </variable>
                <variable name="timeout">20000</variable>
                <variable name="alwaysOnTop">True</variable>
                <variable name="wraplength">160</variable>
              </ProcedureCall>
              <IfSequence id="_2YR1" test="$(ret)&lt;=0">
                <ProcedureCall id="_2YQ+"
                               procedure="startstop.downloadWebswingLocal"/>
                <ElseSequence id="_2YR2">
                  <ServerScriptStep id="_1jxI">
                    <code>rc.logError("Es ist kein Webswing-Demo verfügbar.\n\n" +
            "Setzen Sie bitte im Wurzelknoten dieser Testsuite die Variable \"webswingdir\"\n" +
            "auf das korrekte Verzeichnis und starten Sie den Test neu.")</code>
                  </ServerScriptStep>
                  <ProcedureCall id="_2YR4"
                                 procedure="qfs.utils.testrun.stop.stopTestRun"/>
                </ElseSequence>
              </IfSequence>
            </IfSequence>
          </BasicSequence>
          <BasicSequence id="_2YS2" name="Start Webswing Server local">
            <ProcedureCall id="_2YQC"
                           procedure="startstop.startWebswingServerLocal">
              <variable name="webswingdir">$(webswingdir)</variable>
            </ProcedureCall>
          </BasicSequence>
        </SetupSequence>
        <CleanupSequence id="_2YRQ">
          <TryStep id="_2YS3" name="Stop Webswing Server">
            <ClientStopper client="webswingServer" id="_2YS4"/>
            <ProcessWaiter client="webswingServer" id="_2YS6"
                           timeout="15000"/>
            <CatchSequence exception="TestException" id="_2YS5" maxerror="0"/>
          </TryStep>
          <TryStep id="_2YTf"
                   name="Make sure Webswing client has terminated as well">
            <ClientStopper client="$(client)" id="_2YTg"/>
            <ProcessWaiter client="$(client)" id="_2YTh" timeout="15000"/>
            <CatchSequence exception="TestException" id="_2YTi" maxerror="0"/>
          </TryStep>
        </CleanupSequence>
      </Dependency>
      <Dependency id="_2YOR" name="webswingClientStarted">
        <variable name="url">http://localhost:8080</variable>
        <CommentStep id="_2YTj">
          <heading>&lt;font style="color:green"&gt;The Webswing server must be started first</heading>
        </CommentStep>
        <DependencyReference id="_2YSs"
                             reference="dependencies.webswingServerStarted"/>
        <SetupSequence id="_2YPI" name="Launch Browser if not running">
          <BasicSequence id="_2YRS" name="Set global variables">
            <SetGlobalStep id="_2YRT" varname="webClient">
              <default>$(webClient)</default>
            </SetGlobalStep>
          </BasicSequence>
          <TryStep id="_2YPq" name="Client already started?">
            <ClientWaiter client="$(webClient)" id="_2YPp" timeout="1"/>
            <CatchSequence exception="ClientNotConnectedException" id="_2YPr"
                           maxerror="0"
                           name="Client not started yet -&gt; start ">
              <IfSequence id="_3dr" name="Detect best browser"
                          test="&#34;$(browser)&#34;==&#34;&#34;">
                <ProcedureCall id="_1pIL" local="true"
                               procedure="qfs.web.browser.general.getBestBrowser"
                               retvarname="browser"/>
              </IfSequence>
              <BasicSequence id="_3NC" name="Start Web Engine">
                <BrowserClientStarter browser="$(browser)"
                                      client="$(webClient)"
                                      connectionmode="${default:webmode:$[&#34;Prefer WebDriver&#34; if ${default:preferwebdriver:false} else &#34;Prefer QF-Driver&#34;]}"
                                      executable="${qftest:java}" id="_3ND"
                                      openwindow="false"/>
                <ClientWaiter client="$(webClient)" id="_3NE"/>
              </BasicSequence>
              <BasicSequence id="_1pHV" name="Perform browser settings">
                <ProcedureCall id="_1pHW"
                               procedure="qfs.web.browser.settings.doStartupSettings">
                  <variable name="client">$(webClient)</variable>
