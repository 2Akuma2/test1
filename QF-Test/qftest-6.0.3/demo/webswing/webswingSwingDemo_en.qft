                                  name="Special internal settings for the web client">
                  <code>import de.qfs.lib.log.Log
import de.qfs.lib.log.QFLogger

def logger = new QFLogger ("de.qfs.apps.qftest.client.web.webswingRedirect")

def shouldRedirect = {node, pos -&gt;
    if (node != null) {
        if (logger.level &gt;= Log.MTD) {
            logger.build("shouldRedirect()").add("node: ").add(node).add(", pos:").add(pos).log(Log.MTD);
        }
        def canvases = node.getElementsByTagName("CANVAS")
        if (logger.level &gt;= Log.DBG) {
            logger.build("shouldRedirect()").add("canvases: ").add(canvases).log(Log.DBG);
        }
        if (canvases != null) {
            def hloc = node.getLocationOnScreen()
            if (logger.level &gt;= Log.DBG) {
                logger.build("shouldRedirect()").add("hloc: ").add(hloc).log(Log.DBG);
            }
            def list = new TreeMap ()
            for(def canvas: canvases) {
                def zIndex = -1;
                try {
                    zIndex = canvas.getZIndex();
                    if (zIndex == null || zindex.equals("null")) {
                        zIndex = -1;
                    }
                } catch (Throwable ex) {
                    if (logger.level &gt;= Log.MSG) {
                        logger.log(Log.MSG, "shouldRedirect()", ex)
                    }
                }
                if (logger.level &gt;= Log.DBG) {
                    logger.build("shouldRedirect()").add("canvas: ").add(canvas)
                        .add(", zIndex: ").add(zIndex).log(Log.DBG);
                }
                if (zIndex &gt; 0 || canvases.length == 1) {
                    def cloc = canvas.getLocationOnScreen()
                    if (logger.level &gt;= Log.DBG) {
                        logger.build("shouldRedirect()").add("cloc: ").add(cloc).log(Log.DBG);
                    }
                    if (cloc[2] &gt; 0 &amp;&amp; cloc[3] &gt; 0 /* positive size */) {
                        if (pos[0] &gt;= cloc[0] - hloc[0]
                            &amp;&amp; pos[1] &gt;= cloc[1] - hloc[1]
                            &amp;&amp; pos[0] &lt; cloc[0] - hloc[0] + cloc[2]
                            &amp;&amp; pos[1] &lt; cloc[1] - hloc[1] + cloc[3]) {
                            list.put(zIndex, canvas)
                        }
                    }
                }
            }
            // qf.println(list)
            if (logger.level &gt;= Log.DBG) {
                logger.build("shouldRedirect()").add("list: ").add(list).log(Log.DBG);
            }
            if (list.size() &gt; 0) {
                // list.sort({x, y -&gt; return x.getZIndex() - y.getZIndex()} as Comparator);
                // qf.println(list)
                canvas = list.lastEntry().getValue()
                def cloc = canvas.getLocationOnScreen()
                pos[0] -= cloc[0] - hloc[0]
                pos[1] -= cloc[1] - hloc[1]
                // qf.println(canvas, cloc, hloc, pos)
            if (logger.level &gt;= Log.DBG) {
                logger.build("shouldRedirect()").add("pos: ").add(pos).log(Log.DBG);
            }
                return canvas
            }
        }
    }
    return null
}
resolvers.addRedirectResolver("redirectHTMLtoCANVAS", shouldRedirect, "HTML")

rc.setOption(Options.OPT_JIB_RECORD_WEB_FILTER_JIB_COMPONENTS, true)
rc.setOption(Options.OPT_PLAY_WEB_AUTO_SCROLL, false)
</code>
