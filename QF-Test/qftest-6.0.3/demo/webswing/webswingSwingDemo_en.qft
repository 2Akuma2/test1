    <Package id="_1pOK" name="util">
      <comment>Hilfsmethoden für Dateioperationen.</comment>
      <Procedure id="_q" name="download">
        <variable name="fileURL"/>
        <variable name="targetDir"/>
        <variable name="targetName"/>
        <ServerScriptStep id="_1pPT">
          <code>print("Download Webswing application. This may take a bit of time...")</code>
        </ServerScriptStep>
        <RequestStep id="_1pPS" requestmethod="GET"
                     responsefile="$(targetDir)${system:file.separator}$(targetName)"
                     url="$(fileURL)"/>
      </Procedure>
      <Procedure id="_1pO-" name="entpacken">
        <variable name="file">$(file)</variable>
        <variable name="destination">$(destination)</variable>
        <ServerScriptStep id="_1pPI" name="Archiv entpacken">
          <code>import zipfile

destination = rc.lookup("destination")
zipFile = "%s/%s" % (destination,rc.lookup("file"))

try:
    zip_ref = zipfile.ZipFile(zipFile, 'r')
    fnam = zip_ref.filelist[0].filename
    rc.setLocal("wsdir", fnam.split("/")[0])
    zip_ref.extractall(rc.lookup("destination"))
finally :
    zip_ref.close()
</code>
        </ServerScriptStep>
        <CommentStep id="_2YSN">
          <heading>&lt;font style="color:green"&gt;Move files one directory up</heading>
        </CommentStep>
        <IfSequence id="_1pP7" name="Windows" test="${qftest:windows}">
          <ShellClientStarter client="move"
                              command="powershell move $(destination)\$(wsdir)\* $(destination)"
                              directory="$(webswingdir)" id="_2YR9"/>
          <ProcessWaiter client="move" id="_2YV6" timeout="30000"/>
          <ElseSequence id="_1pP9" name="macOS, Linux">
            <ShellClientStarter client="mv" command="mv $(wsdir)/* ."
                                directory="$(destination)" id="_1pPA"/>
            <ProcessWaiter client="mv" id="_1pPB" timeout="30000"/>
          </ElseSequence>
        </IfSequence>
        <ProcedureCall id="_2YRB" procedure="qfs.shellutils.deleteFile">
          <variable name="file">$(destination)/$(wsdir)</variable>
        </ProcedureCall>
      </Procedure>
