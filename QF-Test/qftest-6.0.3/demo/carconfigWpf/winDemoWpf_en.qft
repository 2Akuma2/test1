<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE RootStep>
<RootStep id="_0" name="root"
          signature="302E021500CAEAB92DF5B530CBFCC2725C756455E25C2EC2EB021500AC2B421DF1E2479FE51C72F370F9EAC974C76919"
          version="6.0.3">
  <include>qfs.qft</include>
  <variable name="buggyMode">false</variable>
  <variable name="executable">"${qftest:suite.dir}/CarConfigWpf.exe"</variable>
  <comment>@author tm, QFS</comment>
  <TestSet id="_1" name="CarConfigurator WPF Demo">
    <DependencyReference id="_3DW"
                         reference="dependencies.sutStartedAndReset"/>
    <TestSet id="_3DX" name="Prices">
      <comment>Check the Prices without specials or accessories</comment>
      <DataDriver id="_3DZ">
        <TableDataBinder id="_3Da" name="models and prices">
          <TableBinderColumn title="refModel" width="174">
            <data>Rolo</data>
            <data>Minigolf</data>
            <data>Rassant</data>
            <data>Rassant Family</data>
            <data>I5</data>
          </TableBinderColumn>
          <TableBinderColumn title="refPrice" width="174">
            <data>$ 12,300.00</data>
            <data>$ 15,000.00</data>
            <data>$ 17,000.00</data>
            <data>$ 18,500.00</data>
            <data>$ 29,000.00</data>
          </TableBinderColumn>
        </TableDataBinder>
      </DataDriver>
      <TestCase id="_3Db" name="Prices in main and options table">
        <TestStep id="_3Fn" name="Price check in main window">
          <MouseEventStep clicks="1" client="$(client)"
                          component="TabbedPane@Vehicles" event="MOUSE_MPRC"
                          id="_2JOZ" modifiers="16"/>
          <ComponentWaiter client="$(client)" component="VehicleTable"
                           id="_2JRG"/>
          <MouseEventStep clicks="1" client="$(client)"
                          component="VehicleTable@Model@$(refModel)"
                          event="MOUSE_MPRC" id="_2JQ0" modifiers="16"/>
          <CheckStringStep client="$(client)" component="CalculatedPrice"
                           id="_2JQ1">
            <text>$(refPrice
)</text>
          </CheckStringStep>
        </TestStep>
        <TestStep id="_3Fo" name="Price check in dialog">
          <ProcedureCall id="_2JRP"
                         procedure="mainwindow.OpenVehiclesDialog"/>
          <MouseEventStep clicks="1" client="$(client)"
                          component="VehiclesTable@Name@$(refModel)"
                          event="MOUSE_MPRC" id="_2JQd" modifiers="16"/>
          <CheckStringStep client="$(client)" component="VehicleName"
                           id="_2JQh">
            <text>$(refModel)</text>
          </CheckStringStep>
          <CheckStringStep client="$(client)" component="VehiclePrice"
                           id="_2JQi">
            <text>$[ "$(refPrice)"[2:] ]</text>
            <comment>check without symbol "$" </comment>
          </CheckStringStep>
          <MouseEventStep clicks="1" client="$(client)"
                          component="CancelButton" event="MOUSE_MPRC"
                          id="_2JRL" modifiers="16"/>
          <ComponentWaiter absence="true" client="$(client)"
                           component="VehiclesDialog" id="_2JRM"/>
        </TestStep>
      </TestCase>
    </TestSet>
    <TestCase id="_3Dn" name="vehicle modifications">
      <TestStep id="_3Do" name="new vehicle">
        <ProcedureCall id="_2JRQ" procedure="mainwindow.OpenVehiclesDialog"/>
        <TextInputStep clear="true" client="$(client)" component="VehicleName"
                       id="_2JQo">
          <text>Testvehicle</text>
        </TextInputStep>
        <TextInputStep clear="true" client="$(client)"
                       component="VehiclePrice" id="_2JQq">
          <text>9999</text>
        </TextInputStep>
        <MouseEventStep clicks="1" client="$(client)" component="NewButton"
                        event="MOUSE_MPRC" id="_2JQs" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="OkButton"
                        event="MOUSE_MPRC" id="_2JQu" modifiers="16"/>
        <ComponentWaiter absence="true" client="$(client)"
                         component="VehiclesDialog" id="_2JRR"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="VehicleTable@Model@Testvehicle"
                        event="MOUSE_MPRC" id="_2JRS" modifiers="16"/>
        <CheckStringStep client="$(client)" component="CalculatedPrice"
                         id="_2JRT">
          <text>$$ 9,999.00</text>
        </CheckStringStep>
      </TestStep>
      <TestStep id="_3D+" name="change vehicle">
        <ProcedureCall id="_2JRb" procedure="mainwindow.OpenVehiclesDialog"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="VehiclesTable@Name@I5" event="MOUSE_MPRC"
                        id="_2JRU" modifiers="16"/>
        <TextInputStep clear="true" client="$(client)"
                       component="VehiclePrice" id="_2JRV" single="false">
          <text>7000</text>
        </TextInputStep>
        <MouseEventStep clicks="1" client="$(client)" component="ChangeButton"
                        event="MOUSE_MPRC" id="_2JRW" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="OkButton"
                        event="MOUSE_MPRC" id="_2JRX" modifiers="16"/>
        <ComponentWaiter absence="true" client="$(client)"
                         component="VehiclesDialog" id="_2JRY"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="VehicleTable@Model@I5" event="MOUSE_MPRC"
                        id="_2JRZ" modifiers="16"/>
        <CheckStringStep client="$(client)" component="CalculatedPrice"
                         id="_2JRa" timeout="5000">
          <text>$$ 7,000.00</text>
        </CheckStringStep>
      </TestStep>
      <TestStep id="_3E8" name="delete vehicle">
        <ProcedureCall id="_2JRc" procedure="mainwindow.OpenVehiclesDialog"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="VehiclesTable@Name@Testvehicle"
                        event="MOUSE_MPRC" id="_2JRd" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="DeleteButton"
                        event="MOUSE_MPRC" id="_2JRe" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="OkButton"
                        event="MOUSE_MPRC" id="_2JRf" modifiers="16"/>
        <ComponentWaiter absence="true" client="$(client)"
                         component="VehiclesDialog" id="_2JRg"/>
        <TryStep id="_2JN+"
                 name="The Testvehicle should not appera in the list anymore">
          <ComponentWaiter client="$(client)"
                           component="VehicleTable@Model@Testvehicle"
                           id="_2JHB" timeout="100"/>
          <ProcedureCall id="_2JO0" procedure="qfs.run-log.logError">
            <variable name="message">Testvehicle is still accessible in list.</variable>
            <variable name="withScreenshots">true</variable>
          </ProcedureCall>
          <CatchSequence exception="IndexNotFoundException" id="_2JN-"
                         maxerror="0"/>
        </TryStep>
      </TestStep>
    </TestCase>
    <TestCase id="_3EG" name="specials including accessories">
      <RepeatSequence count="${default:countSpecials:4}" id="_3EH"
                      name="All specials - if not limited through variable &#34;countSpecials&#34;"
                      var="special">
        <RepeatSequence count="${default:countVehicles:5}" id="_3EI"
                        name="All vehicles - if not limited through &#34;countVehicles&#34;"
                        var="vehicle">
          <variable name="finalPrice"/>
          <ProcedureCall id="_3EJ" procedure="mainwindow.menu.reset"/>
          <ProcedureCall id="_3EK" predelay="1000"
                         procedure="mainwindow.selectVehicle">
            <variable name="vehicle">$(vehicle)</variable>
          </ProcedureCall>
          <ProcedureCall id="_3Fa" procedure="mainwindow.selectSpecial">
            <variable name="special">$(special)</variable>
          </ProcedureCall>
          <ProcedureCall id="_3Fb" local="true"
                         procedure="mainwindow.readFinalPrice"
                         retvarname="finalPrice"/>
          <IfSequence id="_3Fg" name="special is Gomera"
                      test="&#34;$(special)&#34; == &#34;1&#34;">
            <ProcedureCall id="_3Ge"
                           procedure="mainwindow.AccessoriesPanel.selectAccessoriesOfGomera"/>
            <ElseifSequence id="_3EP" name="Special is Jazz"
                            test="&#34;$(special)&#34; == &#34;2&#34;">
              <ProcedureCall id="_3Gf"
                             procedure="mainwindow.AccessoriesPanel.selectAccessoriesOfJazz"/>
            </ElseifSequence>
            <ElseifSequence id="_3ET" name="Special is Luxus"
                            test="&#34;$(special)&#34; == &#34;3&#34;">
              <ProcedureCall id="_3Gg"
                             procedure="mainwindow.AccessoriesPanel.selectAccessoriesOfLuxus"/>
            </ElseifSequence>
          </IfSequence>
          <ProcedureCall id="_3Gk" procedure="mainwindow.checkFinalPrice"/>
        </RepeatSequence>
      </RepeatSequence>
    </TestCase>
  </TestSet>
  <PackageRoot id="_3">
    <Package id="_3Cr" name="dependencies">
      <comment>Package for the dependencies
</comment>
      <Dependency id="_1xuh" name="sutStarted">
        <variable name="program">${qftest:dir.version}/demo/carConfigWpf/CarConfigWpf.exe</variable>
        <SetupSequence id="_1xui"
                       name="some client with WinDriver classes and libraries">
          <SetGlobalStep id="_1xv5" varname="client">
            <default>carconfigWPF</default>
          </SetGlobalStep>
          <TryStep id="_1xuj">
            <ClientWaiter client="$(client)" id="_1xuk" timeout="0"/>
            <CatchSequence exception="ClientNotConnectedException" id="_1xul"
                           maxerror="0">
              <WinClientStarter class="de.qfs.apps.qfwin.QFWin"
                                client="$(client)" id="_1xx0"
                                windowsprogram="$(program)">
                <parameter>-lang</parameter>
                <parameter>en</parameter>
              </WinClientStarter>
              <ClientWaiter client="$(client)" id="_1xuw" timeout="30000"/>
            </CatchSequence>
          </TryStep>
        </SetupSequence>
        <CleanupSequence id="_1xv2">
          <ClientStopper client="$(client)" id="_1xv3"/>
          <ProcessWaiter client="$(client)" id="_1xv4"/>
        </CleanupSequence>
      </Dependency>
      <Dependency id="_2JFh" name="isWindows">
        <comment>Check if the operation system is windows otherwise stop test execution.</comment>
        <SetupSequence id="_2JFg">
          <IfSequence id="_2JFj" test="not ${qftest:windows}">
            <ProcedureCall id="_2JFf"
                           procedure="qfs.dialogs.showInformationMessageDialog">
              <variable name="title">MS Windows only</variable>
              <variable name="msg">This test-suite demonstrates procedures of the 
qfs.autowin package, which works on MS Windows only!</variable>
              <variable name="timeout">20000</variable>
              <variable name="alwaysOnTop">True</variable>
              <variable name="wraplength">160</variable>
            </ProcedureCall>
            <ProcedureCall id="_2JFk"
                           procedure="qfs.utils.testrun.stop.stopTestRun"/>
          </IfSequence>
        </SetupSequence>
      </Dependency>
      <Dependency forcedcleanup="true" id="_3Cw" name="sutStartedAndReset">
        <comment>Starts and reset the SUT to the initial state and set the buggy-flag depending on the global variable "buggyMode"</comment>
        <DependencyReference id="_2JFl" reference="dependencies.isWindows"/>
        <DependencyReference id="_2JFm" reference="dependencies.sutStarted">
          <variable name="program">${qftest:dir.version}/demo/carConfigWpf/CarConfigWpf.exe</variable>
        </DependencyReference>
        <SetupSequence id="_3Aj" name="Reset">
          <ProcedureCall id="_3Ak" procedure="mainwindow.menu.reset"/>
          <ProcedureCall id="_2JPp" procedure="mainwindow.menu.setBuggy">
            <variable name="isBuggy">$(buggyMode)</variable>
          </ProcedureCall>
        </SetupSequence>
      </Dependency>
    </Package>
    <Package id="_3Fp" name="mainwindow">
      <comment>Procedures for actions in main window.</comment>
      <Package id="_3Fq" name="menu">
        <comment>Procedures for menu actions</comment>
        <Procedure id="_38o" name="reset">
          <comment>click File -&gt; Reset to restore the initial state.</comment>
          <ProcedureCall id="_2JRJ"
                         procedure="mainwindow.menu.selectMenuitem">
            <variable name="menu">File</variable>
            <variable name="menuitem">Reset</variable>
          </ProcedureCall>
          <ProcedureCall id="_2JRj" procedure="qfs.win.combobox.setValue">
            <variable name="menu">File</variable>
            <variable name="menuitem">Reset</variable>
            <variable name="id">${id:aComboboxButton}</variable>
            <variable name="listId">${id:aComboboxList}</variable>
            <variable name="value"/>
            <variable name="separator">@</variable>
            <variable name="btnX"/>
            <variable name="btnY"/>
            <variable name="itemX"/>
            <variable name="itemY"/>
          </ProcedureCall>
        </Procedure>
        <Procedure id="_2JPq" name="selectMenuitem">
          <variable name="menu"/>
          <variable name="menuitem"/>
          <comment>Select the menu item from the given menu.
The procedure make use of a generic component.

@param menu		The label of the menu to be selected.
@param menuitem	The label of the menu item to be selected.</comment>
          <SetGlobalStep id="_2JPv" local="true" varname="item">
            <default>$(menu)</default>
          </SetGlobalStep>
          <MouseEventStep clicks="1" client="$(client)"
                          component="genericMenuitem" event="MOUSE_MPRC"
                          id="_2JRH" modifiers="16"/>
          <SetGlobalStep id="_2JRI" local="true" varname="item">
            <default>$(menuitem)</default>
          </SetGlobalStep>
          <MouseEventStep clicks="1" client="$(client)"
                          component="genericMenuitem" event="MOUSE_MPRC"
                          id="_2JPs" modifiers="16"/>
        </Procedure>
        <Procedure id="_1p6t" name="setBuggy">
          <variable name="isBuggy">False</variable>
          <comment>Set the given state to Help -&gt; Buggy.

@param	isBuggy	If set to true, the buggy mode is activated.
</comment>
          <SetGlobalStep id="_2JQj" local="true" varname="item">
            <default>Help</default>
          </SetGlobalStep>
          <MouseEventStep clicks="1" client="$(client)"
                          component="genericMenuitem" event="MOUSE_MPRC"
                          id="_2JPK" modifiers="16"/>
          <TryStep id="_2JPD" name="buggy mode set correcty?">
            <SetGlobalStep id="_2JP+" local="true" varname="item">
              <default>Buggy</default>
            </SetGlobalStep>
            <CheckBooleanStep checktype="checked" client="$(client)"
                              component="genericMenuitem" id="_2JPB"
                              raise="true" state="$(isBuggy)"/>
            <BasicSequence id="_2JPn"
                           name="Close the menu by clicking the menu item again">
              <SetGlobalStep id="_2JQk" local="true" varname="item">
                <default>Help</default>
              </SetGlobalStep>
              <MouseEventStep clicks="1" client="$(client)"
                              component="genericMenuitem" event="MOUSE_MPRC"
                              id="_2JPo" modifiers="16"/>
            </BasicSequence>
            <CatchSequence exception="CheckFailedException" id="_2JPE"
                           maxerror="0">
              <MouseEventStep clicks="1" client="$(client)"
                              component="genericMenuitem" event="MOUSE_MPRC"
                              id="_2JPk" modifiers="16"/>
            </CatchSequence>
          </TryStep>
        </Procedure>
      </Package>
