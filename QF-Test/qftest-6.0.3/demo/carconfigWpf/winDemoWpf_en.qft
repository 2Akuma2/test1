      </Package>
      <Procedure id="_3A9" name="selectVehicle">
        <variable name="vehicle"/>
        <comment>Selects a vehicle

@param vehicle The Index of the vehicle.
</comment>
        <MouseEventStep clicks="1" client="$(client)"
                        component="TabbedPane@Vehicles" event="MOUSE_MPRC"
                        id="_2JOa" modifiers="16" x="10" y="10"/>
        <ComponentWaiter client="$(client)" component="VehicleTable"
                         id="_2JRh"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="VehicleTable@Model&amp;$(vehicle)"
                        event="MOUSE_MPRC" id="_2JRi" modifiers="16"/>
      </Procedure>
      <Procedure id="_3FK" name="selectSpecial">
        <variable name="special"/>
        <comment>Selects a Special

@param special	The index of the special</comment>
        <MouseEventStep clicks="1" client="$(client)"
                        component="TabbedPane@Specials" event="MOUSE_MPRC"
                        id="_2JOc" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="SpecialsCombo" event="MOUSE_MPRC"
                        id="_2JIW" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="SpecialsCombo&amp;$(special)"
                        event="MOUSE_MPRC" id="_2JO+" modifiers="16"/>
      </Procedure>
      <Procedure id="_2J9m" name="readFinalPrice">
        <comment>Reads the final price

@return Returns the final price.</comment>
        <FetchTextStep client="$(client)" component="CalculatedPrice"
                       id="_2JIh" local="true" varname="finalPrice"/>
        <ReturnStep id="_2JAw" retval="$(finalPrice)"/>
      </Procedure>
      <Procedure id="_2JAx" name="checkFinalPrice">
        <variable name="finalPrice"/>
        <comment>Checks the final price.

@param finalPrice	The expected final price</comment>
        <CheckStringStep client="$(client)" component="CalculatedPrice"
                         id="_2JJ7">
          <text>$(finalPrice)</text>
        </CheckStringStep>
      </Procedure>
      <Procedure id="_2JRO" name="OpenVehiclesDialog">
        <ProcedureCall id="_2JRN" procedure="mainwindow.menu.selectMenuitem">
          <variable name="menu">Options</variable>
          <variable name="menuitem">Vehicles...</variable>
        </ProcedureCall>
        <ComponentWaiter client="$(client)" component="VehiclesDialog"
                         id="_2JRK"/>
      </Procedure>
    </Package>
