          <MouseEventStep clicks="1" client="$(client)"
                          component="AccessoryTable@&amp;2" event="MOUSE_MPRC"
                          id="_2JRw" modifiers="16"/>
          <MouseEventStep clicks="1" client="$(client)"
                          component="AccessoryTable@&amp;4" event="MOUSE_MPRC"
