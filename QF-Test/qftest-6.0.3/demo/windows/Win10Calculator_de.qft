  <TestSet id="_2KOL" name="Standard Rechner">
    <TestCase id="_2KOM" name="2+2">
      <TestStep id="_2KQh" name="Öffne den Standard Rechner">
        <ProcedureCall id="_2KTn" procedure="hauptfenster.menü.wähleModus">
          <variable name="modus">Standard</variable>
        </ProcedureCall>
      </TestStep>
      <TestStep id="_2KQi" name="Eingabe 2+2">
        <MouseEventStep clicks="1" client="$(client)" component="2"
                        event="MOUSE_MPRC" id="_2KGK" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="+"
                        event="MOUSE_MPRC" id="_2KGL" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="2"
                        event="MOUSE_MPRC" id="_2KGM" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="="
                        event="MOUSE_MPRC" id="_2KJT" modifiers="16"/>
      </TestStep>
      <TestStep id="_2KQj" name="Prüfe das Ergebnis">
        <CheckStringStep client="$(client)" component="CalculatorResults"
                         id="_2KGO">
          <text>4</text>
        </CheckStringStep>
      </TestStep>
    </TestCase>
    <TestCase id="_2KMn" name="Mit 0 dividieren">
      <TestStep id="_2KQl" name="Eingabe 2/0">
        <MouseEventStep clicks="1" client="$(client)" component="2"
                        event="MOUSE_MPRC" id="_2KMp" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="divide"
                        event="MOUSE_MPRC" id="_2KM-" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="0"
                        event="MOUSE_MPRC" id="_2KN0" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="="
                        event="MOUSE_MPRC" id="_2KN1" modifiers="16"/>
      </TestStep>
      <TestStep id="_2KQm" name="Prüfe das Ergebnis">
        <CheckStringStep client="$(client)" component="CalculatorResults"
                         id="_2KN2">
          <text>Teilen durch 0 nicht möglich</text>
        </CheckStringStep>
      </TestStep>
    </TestCase>
  </TestSet>
  <TestSet id="_2KKh" name="Konverter">
    <TestCase id="_2KLt" name="Länge">
      <TestStep id="_2KQn" name="Öffne den Konverter &#34;Länge&#34;">
        <ProcedureCall id="_2KTc" procedure="hauptfenster.menü.wähleModus">
          <variable name="modus">Länge</variable>
        </ProcedureCall>
      </TestStep>
      <TestStep id="_2KQy" name="Setze das Inputfeld auf Meter">
        <ProcedureCall id="_2KU0"
                       procedure="hauptfenster.setzeEingabeeinheit">
          <variable name="eingabeEinheit">Meter</variable>
        </ProcedureCall>
      </TestStep>
      <TestStep id="_2KQz" name="Setze das Outputfeld auf Zentimeter">
        <ProcedureCall id="_2KU8"
                       procedure="hauptfenster.setzeAusgabeeinheit">
          <variable name="ausgabeEinheit">Zentimeter</variable>
        </ProcedureCall>
      </TestStep>
      <TestStep id="_2KQ+" name="Eingabe 2">
        <MouseEventStep clicks="1" client="$(client)" component="2"
                        event="MOUSE_MPRC" id="_2KPA" modifiers="16"/>
      </TestStep>
      <TestStep id="_2KQ-" name="Prüfe das Ergebnis">
        <CheckStringStep client="$(client)" component="ResultValue"
                         id="_2KPE">
          <text>200</text>
        </CheckStringStep>
        <CheckStringStep client="$(client)" component="label_Yard" id="_2KQu">
          <text>2,19 Yard</text>
        </CheckStringStep>
        <CheckStringStep client="$(client)" component="label_Fuß" id="_2KQv">
          <text>6,56 Fuß</text>
        </CheckStringStep>
        <CheckStringStep client="$(client)" component="label_Zoll" id="_2KQw">
          <text>78,74 Zoll</text>
        </CheckStringStep>
        <CheckStringStep client="$(client)" component="label_Hand" id="_2KQx">
          <text>10,71 Hand</text>
        </CheckStringStep>
      </TestStep>
    </TestCase>
    <TestCase id="_2KJm" name="Volumen">
      <TestStep id="_2KR0" name="Öffne den Konverter &#34;Volumen&#34;">
        <ProcedureCall id="_2KTq" procedure="hauptfenster.menü.wähleModus">
          <variable name="modus">Volumen</variable>
        </ProcedureCall>
      </TestStep>
      <TestStep id="_2KR1" name="Setze das Inputfeld auf Liter">
        <ProcedureCall id="_2KUh"
                       procedure="hauptfenster.setzeEingabeeinheit">
          <variable name="eingabeEinheit">Liter</variable>
        </ProcedureCall>
      </TestStep>
      <TestStep id="_2KR2" name="Setze das Outputfeld auf Milliliter">
        <ProcedureCall id="_2KUi"
                       procedure="hauptfenster.setzeAusgabeeinheit">
          <variable name="ausgabeEinheit">Milliliter</variable>
        </ProcedureCall>
      </TestStep>
      <TestStep id="_2KR3" name="Eingabe 1024">
        <MouseEventStep clicks="1" client="$(client)" component="1"
                        event="MOUSE_MPRC" id="_P" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="0"
                        event="MOUSE_MPRC" id="_Q" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="2"
                        event="MOUSE_MPRC" id="_R" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="4"
                        event="MOUSE_MPRC" id="_S" modifiers="16"/>
      </TestStep>
      <TestStep id="_2KR4" name="Prüfe das Ergebnis">
        <CheckStringStep client="$(client)" component="ResultValue" id="_T"
                         regexp="true">
          <text>.*1\.024\.000.*</text>
        </CheckStringStep>
      </TestStep>
    </TestCase>
  </TestSet>
  <TestSet id="_2KQZ" name="Info">
    <TestCase id="_2KQb" name="Info">
      <TestStep id="_2KR5" name="Öffne den Info Dialog">
        <ProcedureCall id="_2KTm" procedure="hauptfenster.menü.info"/>
      </TestStep>
      <TestStep id="_2KR6" name="Prüfe den Text im Dialog">
        <CheckStringStep client="$(client)" component="AboutContentBody"
                         id="_2KQe" regexp="true">
          <text>(?s).*Microsoft. Alle Rechte vorbehalten.*
</text>
        </CheckStringStep>
        <CheckStringStep client="$(client)" component="Header2" id="_2KQf">
          <text>Info</text>
        </CheckStringStep>
      </TestStep>
      <TestStep id="_2KUa" name="Schließe Menü">
        <MouseEventStep clicks="1" client="$(client)" component="Calculator"
                        event="MOUSE_MPRC" id="_2KUn" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="Menu"
                        event="MOUSE_MPRC" id="_2KUZ" modifiers="16"/>
      </TestStep>
    </TestCase>
  </TestSet>
  <PackageRoot id="_3">
    <Package id="_2KUg" name="StartStop">
      <comment>Prozeduren um die SUT zu starten bzw. zu stoppen.</comment>
      <Procedure id="_2KUc" name="start">
        <comment>Startet SUT.</comment>
        <ShellClientStarter client="killOldCalculator"
                            command="taskkill /F /IM Calculator.exe &gt; nul 2&gt;&amp;1 "
                            id="_2KUW"/>
        <ProcessWaiter client="killOldCalculator" id="_2KUX"/>
        <WinClientStarter class="de.qfs.apps.qfwin.QFWin" client="$(client)"
                          id="_C" windowsprogram="calc.exe"
                          windowtitle="Rechner"/>
        <ClientWaiter client="$(client)" id="_D" timeout="25000"/>
        <ClientScriptStep client="$(client)" id="_1yCQ" interpreter="groovy"
                          name="registerGenericClassNameResolver for MenuItemsHost">
          <code>def getGenericClassName(def winc, def gcln) {
    uid = winc.getUiaId()
    if (uid == "MenuItemsHost") {
        return "List"
    }     
}

resolvers.addResolver("menuitemHostResolver", this.&amp;getGenericClassName, "XAML.Group")</code>
        </ClientScriptStep>
      </Procedure>
      <Procedure id="_2KUe" name="stop">
        <comment>Schließt das Hauptfenster.</comment>
        <TryStep id="_2JPt" name="läuft SUT?">
          <ProcessWaiter client="$(client)" id="_2KCc" timeout="0"/>
          <CatchSequence exception="ClientNotTerminatedException" id="_2JPu"
                         maxerror="0">
            <TryStep id="_2KCa">
              <ClientScriptStep client="$(client)" id="_1yCf"
                                interpreter="groovy"
                                name="unregisterGenericClassNameResolver for MenuItemsHost">
                <code>resolvers.removeResolver("menuitemHostResolver")</code>
              </ClientScriptStep>
              <WindowEventStep client="$(client)" component="Calculator"
                               event="WINDOW_CLOSING" id="_2KQU"/>
              <ProcessWaiter client="$(client)" id="_1xv4"/>
              <CatchSequence exception="TestException" id="_2JlK"
                             maxerror="0">
                <ClientStopper client="$(client)" id="_2KQQ"/>
                <ProcessWaiter client="$(client)" id="_2KQR"/>
              </CatchSequence>
            </TryStep>
          </CatchSequence>
        </TryStep>
      </Procedure>
    </Package>
    <Package id="_2KUb" name="dependencies">
      <comment>Package für die Abhängigkeiten.</comment>
      <Dependency id="_6" name="SUT_Gestartet">
        <comment>Stellt sicher, dass SUT gestartet wurde.</comment>
        <SetupSequence id="_7" name="SUT starten, wenn notwendig">
          <IfSequence id="_2KR7" name="Sprache abfragen"
                      test="'${system:user.language}' != 'de'">
            <ProcedureCall id="_2KRC"
                           procedure="qfs.dialogs.showInformationMessageDialog">
              <variable name="title">Der Test kann nicht ausgeführt werden!</variable>
              <variable name="msg">Diese Testsuite kann nur auf einem deutschen
Windows ausgeführt werden! Bei Windows mit Englisch als Benutzersprache verwenden Sie bitte die Testsuite "Win10Calculator_en.qft".</variable>
              <variable name="timeout">120000</variable>
              <variable name="alwaysOnTop">True</variable>
              <variable name="wraplength">160</variable>
            </ProcedureCall>
            <ProcedureCall id="_2KRD"
                           procedure="qfs.utils.testrun.stop.stopTestRun"/>
          </IfSequence>
          <SetGlobalStep id="_9" varname="client">
            <default>calc</default>
          </SetGlobalStep>
          <ClientWaiter client="$(client)" id="_A" local="true" raise="false"
                        resvarname="isSUTRunning" timeout="1"/>
          <IfSequence id="_B" name="SUT starten, wenn notwendig"
                      test="not $(isSUTRunning)">
            <ProcedureCall id="_2KUd" procedure="StartStop.start"/>
          </IfSequence>
        </SetupSequence>
        <CleanupSequence id="_2JlF" name="SUT schließen">
          <ProcedureCall id="_2KUf" procedure="StartStop.stop"/>
        </CleanupSequence>
      </Dependency>
      <Dependency id="_2KNg" name="SUT_Gestartet_Zurückgesetzt">
        <comment>Stellt sicher, dass SUT gestartet und in den Ausgangszustand gesetzt wurde.</comment>
        <DependencyReference id="_2KRQ"
                             reference="dependencies.SUT_Gestartet"/>
        <SetupSequence id="_2KNh">
          <CheckStringStep client="$(client)" component="Header" id="_2KRN"
                           local="true" resvarname="standard_calc">
            <text>Standard</text>
          </CheckStringStep>
          <IfSequence id="_2KOI"
                      name="Falls nicht der Standard Rechner ausgewählt ist"
                      test="not $(standard_calc)">
            <ProcedureCall id="_2KUV"
                           procedure="hauptfenster.menü.wähleModus">
              <variable name="modus">Standard</variable>
            </ProcedureCall>
          </IfSequence>
          <ProcedureCall id="_2KSh" procedure="hauptfenster.Zurücksetzen"/>
        </SetupSequence>
      </Dependency>
    </Package>
    <Package id="_2KT9" name="hauptfenster">
      <comment>Prozeduren für Aktionen im Hauptfenster.</comment>
      <Package id="_2KTI" name="menü">
        <comment>Prozeduren für Aktionen im Menü.</comment>
        <Procedure id="_2KTl" name="info">
          <comment>Klick auf Menü -&gt; Info.
</comment>
          <MouseEventStep clicks="1" client="$(client)" component="Menu"
                          event="MOUSE_MPRC" id="_2KQc" modifiers="16"/>
          <MouseEventStep clicks="1" client="$(client)"
                          component="AboutButton" event="MOUSE_MPRC"
                          hardevent="true" id="_2KUl" modifiers="16"/>
        </Procedure>
        <Procedure id="_2KTV" name="wähleModus">
          <variable name="modus"/>
          <comment>Wählt den entsprechenden Modus (Standard, Wissenschaftlich, Länge, Temperatur, ...) aus.

@param  modus
@author  QFS</comment>
          <MouseEventStep clicks="1" client="$(client)" component="Menu"
                          event="MOUSE_MPRC" id="_2KOB" modifiers="16"/>
          <MouseEventStep clicks="1" client="$(client)"
                          component="MenuItemsHost@$(modus)"
                          event="MOUSE_MPRC" id="_2KOC" modifiers="16"/>
        </Procedure>
      </Package>
      <Procedure id="_2KPu" name="Zurücksetzen">
        <ComponentWaiter client="$(client)" component="clearButton" id="_2KQF"
                         local="true" raise="false" resvarname="clearButton"
                         timeout="100"/>
        <IfSequence id="_2KQ3" test="$(clearButton)">
          <MouseEventStep clicks="1" client="$(client)"
                          component="clearButton" event="MOUSE_MPRC"
                          id="_2KQ4" modifiers="16"/>
          <ElseSequence id="_2KSW">
            <ComponentWaiter client="$(client)" component="clearEntryButton"
                             id="_2KSa" local="true" raise="false"
                             resvarname="clearButton" timeout="100"/>
            <IfSequence id="_2KSc" test="$(clearButton)">
              <MouseEventStep clicks="1" client="$(client)"
                              component="clearEntryButton" event="MOUSE_MPRC"
                              id="_2KSd" modifiers="16"/>
              <ElseSequence id="_2KSe">
                <MouseEventStep clicks="1" client="$(client)"
                                component="ClearEntryButtonPos0"
                                event="MOUSE_MPRC" id="_2KSg" modifiers="16"/>
              </ElseSequence>
            </IfSequence>
          </ElseSequence>
        </IfSequence>
      </Procedure>
      <Procedure id="_2KU7" name="setzeAusgabeeinheit">
        <variable name="ausgabeEinheit"/>
        <comment>Setzt die Ausgabeeinheit des Konverters.

@param  ausgabeEinheit - die zu wählende Ausgabeeinheit
@author QFS</comment>
        <MouseEventStep clicks="1" client="$(client)" component="Output_Unit"
                        event="MOUSE_MPRC" id="_2KP6" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="Output_Unit@$(ausgabeEinheit)"
                        event="MOUSE_MPRC" id="_2KPU" modifiers="16"/>
        <KeyEventStep client="$(client)" component="Calculator"
                      event="KEY_PTR" id="_2KPK" keychar="9" keycode="9"
                      modifiers="0"/>
      </Procedure>
      <Procedure id="_2KT-" name="setzeEingabeeinheit">
        <variable name="eingabeEinheit"/>
        <comment>Setzt die Eingabeeinheit des Konverters.

@param  eingabeEinheit - die zu wählende Eingabeeinheit

@author  QFS</comment>
        <MouseEventStep clicks="1" client="$(client)" component="Input_Units"
                        event="MOUSE_MPRC" id="_2KPT" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="Input_Units@$(eingabeEinheit)"
                        event="MOUSE_MPRC" id="_2KPG" modifiers="16"/>
        <KeyEventStep client="$(client)" component="Calculator"
                      event="KEY_PTR" id="_2KPJ" keychar="9" keycode="9"
                      modifiers="0"/>
      </Procedure>
    </Package>
  </PackageRoot>
