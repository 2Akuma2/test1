  <include>qfs.qft</include>
  <comment>This is a demo test-suite for the Windows application Calculator.

@author	QFS</comment>
  <DependencyReference id="_2KO3"
                       reference="dependencies.sutStartedAndReset"/>
  <TestSet id="_2KOL" name="Calculator Basic">
    <TestCase id="_2KOM" name="2+2">
      <TestStep id="_2KQh" name="Open standard calculator">
        <ProcedureCall id="_2KUz" procedure="mainwindow.menu.selectMode">
          <variable name="mode">Standard</variable>
        </ProcedureCall>
      </TestStep>
      <TestStep id="_2KQi" name="Input 2+2">
        <MouseEventStep clicks="1" client="$(client)" component="2"
                        event="MOUSE_MPRC" id="_2KGK" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="+"
                        event="MOUSE_MPRC" id="_2KGL" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="2"
                        event="MOUSE_MPRC" id="_2KGM" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="="
                        event="MOUSE_MPRC" id="_2KJT" modifiers="16"/>
      </TestStep>
      <TestStep id="_2KQj" name="Check the result">
        <CheckStringStep client="$(client)" component="CalculatorResults"
                         id="_2KGO">
          <text>4</text>
        </CheckStringStep>
      </TestStep>
    </TestCase>
    <TestCase id="_2KMn" name="Divide 0">
      <TestStep id="_2KQl" name="Input 2/0">
        <MouseEventStep clicks="1" client="$(client)" component="2"
                        event="MOUSE_MPRC" id="_2KMp" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="divide"
                        event="MOUSE_MPRC" id="_2KM-" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="0"
                        event="MOUSE_MPRC" id="_2KN0" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="="
                        event="MOUSE_MPRC" id="_2KN1" modifiers="16"/>
      </TestStep>
      <TestStep id="_2KQm" name="Check the result text">
        <CheckStringStep client="$(client)" component="CalculatorResults"
                         id="_2KN2">
          <text>Cannot divide by zero</text>
        </CheckStringStep>
      </TestStep>
    </TestCase>
  </TestSet>
  <TestSet id="_2KKh" name="Converters">
    <TestCase id="_2KLt" name="Length">
      <TestStep id="_2KQn" name="Open the converter &#34;Length&#34;">
        <ProcedureCall id="_2KU+" procedure="mainwindow.menu.selectMode">
          <variable name="mode">Length</variable>
        </ProcedureCall>
      </TestStep>
      <TestStep id="_2KQy" name="Set input unit to Meters">
        <ProcedureCall id="_2KV0" procedure="mainwindow.setInputunit">
          <variable name="inputUnit">Meters</variable>
        </ProcedureCall>
      </TestStep>
      <TestStep id="_2KQz" name="Set output unit to Centimeters">
        <ProcedureCall id="_2KV1" procedure="mainwindow.setOutputunit">
          <variable name="outputUnit">Centimeters</variable>
        </ProcedureCall>
      </TestStep>
      <TestStep id="_2KQ+" name="Input 2">
        <MouseEventStep clicks="1" client="$(client)" component="2"
                        event="MOUSE_MPRC" id="_2KPA" modifiers="16"/>
      </TestStep>
      <TestStep id="_2KQ-" name="Check the result">
        <CheckStringStep client="$(client)" component="ResultValue"
                         id="_2KPE">
          <text>200</text>
        </CheckStringStep>
        <CheckStringStep client="$(client)" component="label_Yards"
                         id="_2KQu">
          <text>2.19 Yards</text>
        </CheckStringStep>
        <CheckStringStep client="$(client)" component="label_Feet" id="_2KQv">
          <text>6.56 Feet</text>
        </CheckStringStep>
        <CheckStringStep client="$(client)" component="label_Inches"
                         id="_2KQw">
          <text>78.74 Inches</text>
        </CheckStringStep>
        <CheckStringStep client="$(client)" component="label_Hands"
                         id="_2KQx">
          <text>10.71 hands</text>
        </CheckStringStep>
      </TestStep>
    </TestCase>
    <TestCase id="_2KJm" name="Volume">
      <TestStep id="_2KR0" name="Open the converter &#34;Volume&#34;">
        <ProcedureCall id="_2KU-" procedure="mainwindow.menu.selectMode">
          <variable name="mode">Volume</variable>
        </ProcedureCall>
      </TestStep>
      <TestStep id="_2KR1" name="Set input unit to Liter">
        <ProcedureCall id="_2KV2" procedure="mainwindow.setInputunit">
          <variable name="inputUnit">Liters</variable>
        </ProcedureCall>
      </TestStep>
      <TestStep id="_2KR2" name="Set output unit to Milliliter">
        <ProcedureCall id="_2KV3" procedure="mainwindow.setOutputunit">
          <variable name="outputUnit">Milliliters</variable>
        </ProcedureCall>
      </TestStep>
      <TestStep id="_2KR3" name="Input 1024">
        <MouseEventStep clicks="1" client="$(client)" component="1"
                        event="MOUSE_MPRC" id="_P" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="0"
                        event="MOUSE_MPRC" id="_Q" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="2"
                        event="MOUSE_MPRC" id="_R" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)" component="4"
                        event="MOUSE_MPRC" id="_S" modifiers="16"/>
      </TestStep>
      <TestStep id="_2KR4" name="Check the result">
        <CheckStringStep client="$(client)" component="ResultValue" id="_T">
          <text>1,024,000</text>
        </CheckStringStep>
      </TestStep>
    </TestCase>
  </TestSet>
  <TestSet id="_2KQZ" name="About">
    <TestCase id="_2KQb" name="About">
      <TestStep id="_2KR5" name="Open the About dialog">
        <ProcedureCall id="_2KVC" procedure="mainwindow.menu.about"/>
      </TestStep>
      <TestStep id="_2KR6" name="Check the text in the dialog">
        <CheckStringStep client="$(client)" component="AboutContentBody"
                         id="_2KQe" regexp="true">
          <text>(?s).*Microsoft.*All rights reserved.*</text>
        </CheckStringStep>
        <CheckStringStep client="$(client)" component="Header2" id="_2KQf">
          <text>About</text>
        </CheckStringStep>
      </TestStep>
    </TestCase>
  </TestSet>
  <PackageRoot id="_3">
    <Package id="_2KUY" name="dependencies">
      <comment>Package for dependencies.</comment>
      <Dependency id="_6" name="sutStarted">
        <comment>Ensure the SUT is started.</comment>
        <SetupSequence id="_7" name="Start SUT, if required">
          <IfSequence id="_2KR7" name="Language detection"
                      test="'${system:user.language}' != 'en'">
            <ProcedureCall id="_2KRC"
                           procedure="qfs.dialogs.showInformationMessageDialog">
              <variable name="title">The test can not be executed!</variable>
              <variable name="msg">The test-suite only works for Windows with English as user language.
In case you have a German Windows please run the testsuite "Win10Calculator_de.qft".</variable>
              <variable name="timeout">120000</variable>
              <variable name="alwaysOnTop">True</variable>
              <variable name="wraplength">160</variable>
            </ProcedureCall>
            <ProcedureCall id="_2KRD"
                           procedure="qfs.utils.testrun.stop.stopTestRun"/>
          </IfSequence>
          <SetGlobalStep id="_9" varname="client">
            <default>calc</default>
          </SetGlobalStep>
          <ClientWaiter client="$(client)" id="_A" local="true" raise="false"
                        resvarname="isSUTRunning" timeout="1"/>
          <IfSequence id="_B" name="Launch SUT if not running"
                      test="not $(isSUTRunning)">
            <ShellClientStarter client="killOldCalculator"
                                command="taskkill /F /IM Calculator.exe &gt; nul 2&gt;&amp;1 "
                                id="_2KUW"/>
            <ProcessWaiter client="killOldCalculator" id="_2KUX"/>
            <WinClientStarter class="de.qfs.apps.qfwin.QFWin"
                              client="$(client)" id="_C"
                              windowsprogram="calc.exe"
                              windowtitle="Calculator"/>
            <ClientWaiter client="$(client)" id="_D" timeout="25000"/>
            <ClientScriptStep client="$(client)" id="_1yCQ"
                              interpreter="groovy"
                              name="registerGenericClassNameResolver for MenuItemsHost">
              <code>def getGenericClassName(def winc, def gcln) {
    uid = winc.getUiaId()
    if (uid == "MenuItemsHost") {
        return "List"
    }     
}

resolvers.addResolver("menuitemHostResolver", this.&amp;getGenericClassName, "XAML.Group")</code>
            </ClientScriptStep>
          </IfSequence>
        </SetupSequence>
        <CleanupSequence id="_2JlF" name="Close SUT">
          <TryStep id="_2JPt" name="stopped?">
            <ProcessWaiter client="$(client)" id="_2KCc" timeout="0"/>
            <CatchSequence exception="ClientNotTerminatedException" id="_2JPu"
                           maxerror="0">
              <TryStep id="_2KCa">
                <ClientScriptStep client="$(client)" id="_1yCf"
                                  interpreter="groovy"
                                  name="unregisterGenericClassNameResolver for MenuItemsHost">
                  <code>resolvers.removeResolver("menuitemHostResolver")</code>
                </ClientScriptStep>
                <WindowEventStep client="$(client)" component="Calculator"
                                 event="WINDOW_CLOSING" id="_2KQU"/>
                <ProcessWaiter client="$(client)" id="_1xv4"/>
                <CatchSequence exception="TestException" id="_2JlK"
                               maxerror="0">
                  <ClientStopper client="$(client)" id="_2KQQ"/>
                  <ProcessWaiter client="$(client)" id="_2KQR"/>
                </CatchSequence>
              </TryStep>
            </CatchSequence>
          </TryStep>
        </CleanupSequence>
      </Dependency>
      <Dependency id="_2KNg" name="sutStartedAndReset">
        <DependencyReference id="_2KRQ" reference="dependencies.sutStarted"/>
        <SetupSequence id="_2KNh">
          <CheckStringStep client="$(client)" component="Header" id="_2KRN"
