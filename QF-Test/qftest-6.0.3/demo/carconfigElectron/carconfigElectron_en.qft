      <Procedure id="_3dg" name="terminate">
        <comment>Terminate the SUT.</comment>
        <TryStep id="_3dl" name="already stopped? ">
          <ProcessWaiter client="$(client)" id="_3dk" timeout="1"/>
          <CatchSequence exception="ClientNotTerminatedException" id="_3e3"
                         maxerror="0" name="not stopped -&gt; stop it">
            <TryStep id="_3dz" name="Close browser window">
              <WindowEventStep client="$(client)" component="CarConfig"
                               event="WINDOW_CLOSING" id="_3e4"/>
              <ProcessWaiter client="$(client)" id="_3e5"/>
              <CatchSequence exception="TestException" id="_3e6" maxerror="0"
                             name="Couldn't close browser window -&gt; kill process">
                <ClientStopper client="$(client)" id="_3e7"/>
                <ProcessWaiter client="$(client)" id="_3e8"/>
              </CatchSequence>
            </TryStep>
          </CatchSequence>
          <FinallySequence id="_1pGe" name="Reset server options">
            <ServerScriptStep id="_1pGf"
                              name="Reset option to start web engine in special classloader">
              <code>rc.unsetOption(Options.OPT_WEB_USE_CONNECTOR_LOADER)</code>
            </ServerScriptStep>
          </FinallySequence>
        </TryStep>
      </Procedure>
    </Package>
    <Package id="_1pOK" name="util">
      <comment>Hilfsmethoden für Dateioperationen.</comment>
      <Procedure id="_q" name="download">
        <variable name="fileURL"/>
        <variable name="targetDir"/>
        <variable name="targetName"/>
        <ServerScriptStep id="_1pPT">
          <code>print("Download Electron CarConfig Demo. This might take some time...")</code>
        </ServerScriptStep>
        <RequestStep id="_1pPS" requestmethod="GET"
                     responsefile="$(targetDir)${system:file.separator}$(targetName)"
                     url="$(fileURL)"/>
      </Procedure>
      <Procedure id="_1pO-" name="unzip">
        <variable name="file">$(file)</variable>
        <variable name="destination">$(destination)</variable>
        <IfSequence id="_1pP7" name="Windows" test="${qftest:windows}">
          <ServerScriptStep id="_1pPI" name="Extract zipfile">
            <code>import zipfile

destination = rc.lookup("destination")
zipFile = "%s/%s" % (destination,rc.lookup("file"))

try:
    zip_ref = zipfile.ZipFile(zipFile, 'r')
    zip_ref.extractall(rc.lookup("destination"))
finally :
    zip_ref.close()
</code>
          </ServerScriptStep>
          <ElseSequence id="_1pP9" name="macOS, Linux">
            <ShellClientStarter client="unzip"
                                command="/usr/bin/unzip &#34;$(file)&#34;"
                                directory="$(destination)" id="_1pPA"/>
            <ProcessWaiter client="unzip" id="_1pPB" timeout="60000"/>
          </ElseSequence>
        </IfSequence>
      </Procedure>
    </Package>
