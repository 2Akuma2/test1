  <comment>This test-suite contains a  number of sample scenarios and work flows for the Electron/HTML-based car configuration demo application that is also used and described in the advanced tutorial. Basic and advanced QF-Test techniques are used like test-sets, test-cases, dependencies, procedures, data-drivers, nested loops, try/catch.

@param	buggyMode    If you set this variable to true some test-cases will fail, so you can check the error reporting of QF-Test.

@author QFS</comment>
  <TestSet id="_1" name="CarConfiguratorElectron Demo">
    <DependencyReference id="_3G4"
                         reference="dependencies.sutStartedAndReset"/>
    <TestSet id="_K" name="Model prices">
      <comment>Check the standard prices.</comment>
      <TestSet id="_1pPg" name="Prices without accessories">
        <comment>Check the prices without accessories.</comment>
        <DataDriver id="_1pPh">
          <TableDataBinder id="_1pPi" name="Models and prices">
            <TableBinderColumn title="refModel" width="71">
              <data>Rolo</data>
              <data>Minigolf</data>
              <data>Rassant</data>
              <data>Rassant Family</data>
              <data>I5</data>
            </TableBinderColumn>
            <TableBinderColumn title="refPrice" width="217">
              <data>$$12,300.00</data>
              <data>$$15,000.00</data>
              <data>$$17,000.00</data>
              <data>$$18,500.00</data>
              <data>$$29,000.00</data>
            </TableBinderColumn>
          </TableDataBinder>
        </DataDriver>
        <TestCase id="_1pPj" name="Prices in main and options table">
          <variable name="index"/>
          <TestStep id="_1pPk" name="check of price in main window">
            <MouseEventStep clicks="1" client="$(client)"
                            component="TabbedPane@Vehicles" event="MOUSE_MPRC"
                            id="_3WH" modifiers="16"/>
            <FetchIndexStep client="$(client)"
                            component="VehicleTable@Model@$(refModel)"
                            id="_1pGh" local="true" varname="index"/>
            <CheckStringStep client="$(client)"
                             component="VehicleTable@Price&amp;$(index)"
                             id="_3Mr">
              <text>$(refPrice)</text>
            </CheckStringStep>
            <MouseEventStep clicks="1" client="$(client)"
                            component="VehicleTable@Price&amp;$(index)"
                            event="MOUSE_MPRC" id="_3YL" modifiers="16"/>
            <CheckStringStep client="$(client)" component="BasePrice"
                             id="_3NX" timeout="500">
              <text>$(refPrice)</text>
            </CheckStringStep>
            <CheckStringStep client="$(client)" component="CalculatedPrice"
                             id="_3Ng">
              <text>$(refPrice)</text>
            </CheckStringStep>
          </TestStep>
          <TestStep id="_1pPq" name="check of price in vehicles dialog">
            <SelectionEventStep client="$(client)" component="CarConfig"
                                detail="clickmenu:@Options/Vehicles..."
                                event="SELECTION" id="_1pPu"/>
            <MouseEventStep clicks="1" client="$(client)"
                            component="VehiclesTable@Price&amp;$(index)"
                            event="MOUSE_MPRC" id="_3Zc" modifiers="16" x="73"
                            y="10"/>
            <CheckStringStep client="$(client)"
                             component="VehiclesTable@Price&amp;$(index)"
                             id="_3Dk" timeout="500">
              <text>$(refPrice)</text>
            </CheckStringStep>
            <MouseEventStep clicks="1" client="$(client)"
                            component="CancelButton" event="MOUSE_MPRC"
                            id="_3Zf" modifiers="16"/>
          </TestStep>
        </TestCase>
      </TestSet>
    </TestSet>
