        <discriminator>browserMac</discriminator>
        <discriminator>browserLinux</discriminator>
        <comment>Ensure the SUT is started.</comment>
        <SetupSequence id="_3Ac" name="Start SUT, if required">
          <SetGlobalStep id="_3OU" varname="client">
