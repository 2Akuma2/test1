      <Procedure id="_3A9" name="wähleFahrzeug">
        <variable name="fahrzeug"/>
        <comment>Wählt ein Fahrzeug

@param fahrzeug	Der Index des Fahrzeuges.
</comment>
        <MouseEventStep clicks="1" client="$(client)"
                        component="TabbedPane@Fahrzeuge" event="MOUSE_MPRC"
                        id="_1pRx" modifiers="16"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="VehicleTable@Modell&amp;$(fahrzeug)"
                        event="MOUSE_MPRC" id="_1pRy" modifiers="16"/>
      </Procedure>
      <Procedure id="_3FK" name="wähleSondermodell">
        <variable name="sondermodell"/>
        <comment>Wähle ein Sondermodell.

@param sondermodell	Der Index des Sondermodells</comment>
        <MouseEventStep clicks="1" client="$(client)"
                        component="TabbedPane@Sondermodelle"
                        event="MOUSE_MPRC" id="_1pRz" modifiers="16"/>
        <SelectionEventStep client="$(client)"
                            component="SpecialsCombo&amp;$(sondermodell)"
                            detail="0" event="SELECTION" id="_1pR+"/>
      </Procedure>
      <Procedure id="_3FS" name="leseEndpreis">
        <comment>Lese den Endpreis.

@return Gibt den Endpreis zurück.</comment>
