          <MouseEventStep clicks="1" client="$(client)"
                          component="VehiclePrice" event="MOUSE_MPRC"
                          id="_3iL" modifiers="16"/>
          <TextInputStep clear="true" client="$(client)"
                         component="VehiclePrice" id="_3gx">
            <text>7000</text>
          </TextInputStep>
          <MouseEventStep clicks="1" client="$(client)"
                          component="ChangeButton" event="MOUSE_MPRC"
                          id="_3fx" modifiers="16"/>
          <MouseEventStep clicks="1" client="$(client)" component="OkButton"
                          event="MOUSE_MPRC" id="_3gy" modifiers="16"/>
          <MouseEventStep clicks="1" client="$(client)"
                          component="VehicleTable@Preis&amp;2"
                          event="MOUSE_MPRC" id="_3bS" modifiers="16"/>
          <CheckStringStep client="$(client)" component="BasePrice" id="_3SL"
