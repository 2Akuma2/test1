            <SelectionEventStep client="$(client)" component="CarConfig"
                                detail="clickmenu:@CarConfig/Über CarConfig"
                                event="SELECTION" id="_1pNx"/>
            <ElseSequence id="_1pNy">
              <SelectionEventStep client="$(client)" component="CarConfig"
                                  detail="clickmenu:@Hilfe/Info"
                                  event="SELECTION" id="_1pNz"/>
