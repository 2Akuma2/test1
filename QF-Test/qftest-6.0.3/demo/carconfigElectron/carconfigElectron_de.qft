<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE RootStep>
<RootStep id="_0" name="root"
          signature="302C02140C3CC889363491ECD1B959DB4773936E2D14586A021444A5369623321714E9E8736138E0BBA482069C3D"
          version="6.0.3">
  <include>qfs.qft</include>
  <reference>qfs.qft</reference>
  <variable name="buggyMode">False</variable>
  <variable name="carConfigVersion">1.1.0</variable>
