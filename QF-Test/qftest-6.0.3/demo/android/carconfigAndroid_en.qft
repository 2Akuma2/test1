  <TestSet id="_1" name="Android CarConfigurator Demo">
    <TestCase id="_2YjS" name="Ensure correct language">
      <DependencyReference id="_2YjT" reference="Dependencies.SUT started">
        <variable name="deviceName">&amp;0</variable>
      </DependencyReference>
      <ComponentWaiter client="$(client)" component="textfieldBenutzer"
                       id="_2Yib" local="true" raise="false"
                       resvarname="wrongLanguage" timeout="1"/>
      <IfSequence id="_2Yic" test="$(wrongLanguage)">
        <ProcedureCall id="_2Yig" procedure="Start-Stop.wrong language"/>
      </IfSequence>
    </TestCase>
    <TestSet id="_3DY" name="Model prices">
      <comment>Check the standard prices.</comment>
      <DependencyReference id="_2YTb"
                           reference="Dependencies.SUT started and user logged in"/>
      <TestCase id="_3Db" name="Check prices">
        <variable name="index"/>
        <TestStep id="_2YhA" name="Check selected prices ">
          <DataDriver id="_3DZ">
            <TableDataBinder id="_3Da" intervals="0-2"
                             name="Models and Prices">
              <TableBinderColumn title="refModel" width="546">
                <data>Rolo</data>
                <data>Minigolf</data>
                <data>Rassant</data>
                <data>Rassant Family</data>
                <data>I5</data>
              </TableBinderColumn>
              <TableBinderColumn title="refPrice" width="546">
                <data>$ 12,300.00</data>
                <data>$ 15,000.00</data>
                <data>$ 17,000.00</data>
                <data>$ 18,500.00</data>
                <data>$ 29,000.00</data>
              </TableBinderColumn>
            </TableDataBinder>
          </DataDriver>
          <BasicSequence id="_3Fn" name="Check prices on main screen">
            <MouseEventStep clicks="1" client="$(client)"
                            component="tabpanel@VEHICLES" event="MOUSE_MPRC"
                            id="_2Ye8" modifiers="16"/>
            <MouseEventStep clicks="1" client="$(client)"
                            component="buttonHIDE" event="MOUSE_MPRC"
                            id="_2YeO" modifiers="16"/>
            <MouseEventStep clicks="1" client="$(client)"
                            component="list2@$(refModel)" event="MOUSE_MPRC"
                            id="_2YeF" modifiers="16"/>
            <MouseEventStep clicks="1" client="$(client)"
                            component="buttonEXPAND" event="MOUSE_MPRC"
                            id="_2YeN" modifiers="16"/>
            <CheckStringStep client="$(client)"
                             component="textfieldBase_price" id="_2YeE">
              <text>$(refPrice)</text>
            </CheckStringStep>
          </BasicSequence>
        </TestStep>
      </TestCase>
    </TestSet>
    <TestSet id="_2Yf0" name="Options">
      <DependencyReference id="_2Yfg"
                           reference="Dependencies.Options panel is opened"/>
      <TestCase id="_2YhR" name="Change vehicles">
        <BasicSequence id="_2YhS" name="New">
          <MouseEventStep clicks="1" client="$(client)"
                          component="iconMore_options" event="MOUSE_MPRC"
                          id="_2Yfr" modifiers="16"/>
          <MouseEventStep clicks="1" client="$(client)" component="list@New"
                          event="MOUSE_MPRC" id="_2Yfs" modifiers="16"/>
          <TextInputStep client="$(client)" component="textfieldVehicle_name"
                         id="_2Yfz">
            <text>Test</text>
          </TextInputStep>
          <KeyEventStep client="$(client)" component="textfieldVehicle_name"
                        event="KEY_PTR" id="_2Yiw" keychar="10" keycode="10"
                        modifiers="0"/>
          <TextInputStep client="$(client)" component="textfieldPrice"
                         id="_2Yg5">
            <text>9999</text>
          </TextInputStep>
          <KeyEventStep client="$(client)" component="textfieldPrice"
                        event="KEY_PTR" id="_2Yix" keychar="10" keycode="10"
                        modifiers="0"/>
          <MouseEventStep clicks="1" client="$(client)"
                          component="buttonFINISH" event="MOUSE_MPRC"
                          id="_2Yg8" modifiers="16"/>
          <CheckStringStep client="$(client)" component="list2@Test@#Label:"
                           id="_2YgA">
            <text>Test</text>
          </CheckStringStep>
          <CheckStringStep client="$(client)"
                           component="list2@Test@#Label:&lt;2&gt;" id="_2YgB">
            <text>$$ 9,999.00</text>
          </CheckStringStep>
        </BasicSequence>
        <BasicSequence id="_2YhT" name="Change">
          <CommentStep id="_2Ygr">
            <heading>&lt;font style="color:green"&gt;A right click in Android means long click</heading>
          </CommentStep>
          <MouseEventStep clicks="1" client="$(client)" component="list2@Test"
                          event="MOUSE_MPRC" id="_2Ygp" modifiers="4"/>
          <MouseEventStep clicks="1" client="$(client)"
                          component="#Label:CHANGE" event="MOUSE_MPRC"
                          id="_2Ygq" modifiers="16"/>
          <TextInputStep clear="true" client="$(client)"
                         component="textfieldPrice" id="_2Ygu">
            <text>7000</text>
          </TextInputStep>
          <KeyEventStep client="$(client)" component="textfieldPrice"
                        event="KEY_PTR" id="_2Yiy" keychar="10" keycode="10"
                        modifiers="0"/>
          <MouseEventStep clicks="1" client="$(client)" component="#FINISH"
                          event="MOUSE_MPRC" id="_2Ygw" modifiers="16"/>
          <CheckStringStep client="$(client)" component="list2@Test@#Label:"
                           id="_2Ygy">
            <text>Test</text>
          </CheckStringStep>
          <CheckStringStep client="$(client)"
                           component="list2@Test@#Label:&lt;2&gt;" id="_2Ygz">
            <text>$$ 7,000.00</text>
          </CheckStringStep>
        </BasicSequence>
        <BasicSequence id="_2YhU" name="Delete">
          <MouseEventStep clicks="1" client="$(client)" component="list2@Test"
                          event="MOUSE_MPRC" id="_2Ygs" modifiers="4"/>
          <MouseEventStep clicks="1" client="$(client)" component="#DELETE"
                          event="MOUSE_MPRC" id="_2Ygt" modifiers="16"/>
          <ComponentWaiter absence="true" client="$(client)"
                           component="list2@Test" id="_2Yh3" raise="false"
                           timeout="5000"/>
        </BasicSequence>
      </TestCase>
    </TestSet>
  </TestSet>
  <PackageRoot id="_3">
